
public class Main{
    public static void main(String[] args){
        int arr[] = {12,44,55,33,5,23,65,45,23};
        int max = findMax(arr);
        System.out.println("The Maximum value is : "+max);
    }
    public static int findMax(int[] arr){
        int max = arr[0];
        for(int i = 1; i < arr.length; i++){
            if(arr[i] > max){
                max = arr[i];
            }
        }
        return max;
    }
}
